extends layout

block content
    #carouselExampleIndicators.carousel.slide(data-ride='carousel')
        ol.carousel-indicators
            li.active(data-target='#carouselExampleIndicators', data-slide-to='0')
            li(data-target='#carouselExampleIndicators', data-slide-to='1')
            li(data-target='#carouselExampleIndicators', data-slide-to='2')
        .carousel-inner(role='listbox')
            .carousel-item.active
                img.d-block.img-fluid(src='http://www.godinguitars.com/guitars_15/multiac_grcon_duet/multiac_grcon_duet_32495.jpg', alt='First slide')
            .carousel-item
                img.d-block.img-fluid(src='http://www.godinguitars.com/guitars_15/shifter_bass/36141_shifter4_black_rn.jpg', alt='Second slide')
            .carousel-item
                img.d-block.img-fluid(src='http://www.simonandpatrick.com/guitars/38084_woodland_pro_folk_mahogany.jpg', alt='Third slide')
            .carousel-item
                img.d-block.img-fluid(src='http://www.godinguitars.com/guitars_15/summit_classic_ltd_cognac/42623_summit_classic_ltd_cognac.jpg', alt='Fourth slide')
            .carousel-item
                img.d-block.img-fluid(src='http://www.simonandpatrick.com/guitars/40469_s&p_cw_gt_concerthall.jpg', alt='Fifth slide')
        a.carousel-control-prev(href='#carouselExampleIndicators', role='button', data-slide='prev')
            span.carousel-control-prev-icon(aria-hidden='true')
            span.sr-only Previous
        a.carousel-control-next(href='#carouselExampleIndicators', role='button', data-slide='next')
            span.carousel-control-next-icon(aria-hidden='true')
            span.sr-only Next
